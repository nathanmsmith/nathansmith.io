{
  "name": "nathansmith.io",
  "description": "Nathan Smith in HTML",
  "author": "Nathan Smith <nathan@nathansmith.io>",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "gatsby develop -p 1234",
    "build": "gatsby build",
    "serve": "gatsby serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,md,json,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.2",
    "@fortawesome/free-brands-svg-icons": "5.2.0",
    "@fortawesome/free-solid-svg-icons": "5.2.0",
    "@fortawesome/react-fontawesome": "0.1.1",
    "@types/react-helmet": "5.0.7",
    "@types/react-modal": "3.2.0",
    "emotion": "9.2.6",
    "emotion-server": "9.2.6",
    "gatsby": "next",
    "gatsby-image": "next",
    "gatsby-plugin-emotion": "next",
    "gatsby-plugin-google-analytics": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-sharp": "next",
    "gatsby-plugin-typescript": "next",
    "gatsby-plugin-typography": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "gatsby-transformer-sharp": "next",
    "polished": "1.9.3",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-emotion": "9.2.6",
    "react-helmet": "5.2.0",
    "react-modal": "3.5.1",
    "react-typography": "0.16.13",
    "rehype-react": "3.0.3",
    "typescript": "3.0.1",
    "typography": "0.16.17"
  },
  "devDependencies": {
    "husky": "1.0.0-rc.13",
    "lint-staged": "7.2.2",
    "prettier": "1.14.2"
  }
}
