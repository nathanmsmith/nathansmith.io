{
  "name": "nathansmith.io",
  "description": "Nathan Smith in HTML",
  "author": "Nathan Smith <nathan@nathansmith.io>",
  "version": "1.0.0",
  "scripts": {
    "start": "gatsby develop -p 1234",
    "build": "gatsby build",
    "serve": "gatsby serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,md,json,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "emotion": "9.2.5",
    "emotion-server": "9.2.5",
    "gatsby": "next",
    "gatsby-image": "next",
    "gatsby-plugin-emotion": "next",
    "gatsby-plugin-google-analytics": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-sharp": "next",
    "gatsby-plugin-typescript": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "gatsby-transformer-sharp": "next",
    "polished": "1.9.3",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-emotion": "9.2.5",
    "react-helmet": "5.2.0",
    "rehype-react": "3.0.2",
    "typescript": "2.9.2"
  },
  "devDependencies": {
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0"
  }
}
