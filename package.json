{
  "name": "nathansmith.io",
  "description": "Nathan Smith in HTML",
  "author": "Nathan Smith <nathan@nathansmith.io>",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "gatsby develop --open -p 1234",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "rm -rf node_modules/ .cache/ public/",
    "typegen": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,md,json,js,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-brands-svg-icons": "5.15.1",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.11",
    "@fullhuman/postcss-purgecss": "1.3.0",
    "@graphql-codegen/cli": "1.19.0",
    "@mosch/gatsby-source-github": "0.4.1",
    "@types/react": "16.9.53",
    "@types/react-helmet": "5.0.16",
    "@types/react-modal": "3.10.6",
    "autoprefixer": "9.8.6",
    "dotenv": "8.2.0",
    "gatsby": "2.24.85",
    "gatsby-image": "2.4.21",
    "gatsby-plugin-feed": "2.5.19",
    "gatsby-plugin-google-analytics": "2.3.19",
    "gatsby-plugin-manifest": "2.4.35",
    "gatsby-plugin-netlify": "2.3.24",
    "gatsby-plugin-netlify-cache": "1.2.0",
    "gatsby-plugin-postcss": "2.3.13",
    "gatsby-plugin-react-helmet": "3.3.14",
    "gatsby-plugin-react-helmet-canonical-urls": "1.4.0",
    "gatsby-plugin-remove-trailing-slashes": "2.3.13",
    "gatsby-plugin-sass": "2.3.21",
    "gatsby-plugin-sharp": "2.6.43",
    "gatsby-plugin-typescript": "2.4.24",
    "gatsby-plugin-typography": "2.5.13",
    "gatsby-remark-autolink-headers": "2.3.15",
    "gatsby-remark-images": "3.3.39",
    "gatsby-remark-katex": "3.3.14",
    "gatsby-remark-prismjs": "3.5.16",
    "gatsby-remark-smartypants": "2.3.13",
    "gatsby-source-filesystem": "2.3.35",
    "gatsby-transformer-remark": "2.8.46",
    "gatsby-transformer-sharp": "2.5.19",
    "katex": "0.12.0",
    "node-sass": "4.14.1",
    "postcss-nested": "4.2.3",
    "prismjs": "1.19.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-helmet": "5.2.1",
    "react-modal": "3.11.2",
    "react-typography": "0.16.19",
    "rehype-react": "4.0.1",
    "tailwindcss": "1.9.6",
    "typescript": "3.9.7",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1"
  }
}
