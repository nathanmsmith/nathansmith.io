{
  "name": "nathansmith.io",
  "description": "Nathan Smith in HTML",
  "author": "Nathan Smith <nathan@nathansmith.io>",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "gatsby develop --open -p 1234",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "rm -rf node_modules/ .cache/ public/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,md,json,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@emotion/core": "10.0.5",
    "@emotion/styled": "10.0.5",
    "@fortawesome/fontawesome-svg-core": "1.2.12",
    "@fortawesome/free-brands-svg-icons": "5.6.3",
    "@fortawesome/free-solid-svg-icons": "5.6.3",
    "@fortawesome/react-fontawesome": "0.1.3",
    "@types/react": "16.7.18",
    "@types/react-helmet": "5.0.7",
    "@types/react-modal": "3.6.0",
    "emotion": "10.0.5",
    "emotion-server": "10.0.5",
    "gatsby": "2.0.76",
    "gatsby-image": "2.0.22",
    "gatsby-plugin-emotion": "4.0.1",
    "gatsby-plugin-google-analytics": "2.0.8",
    "gatsby-plugin-manifest": "2.0.12",
    "gatsby-plugin-netlify": "2.0.6",
    "gatsby-plugin-react-helmet": "3.0.5",
    "gatsby-plugin-remove-trailing-slashes": "2.0.6",
    "gatsby-plugin-sharp": "2.0.16",
    "gatsby-plugin-typescript": "2.0.3",
    "gatsby-plugin-typography": "2.2.4",
    "gatsby-source-filesystem": "2.0.12",
    "gatsby-transformer-remark": "2.1.17",
    "gatsby-transformer-sharp": "2.1.9",
    "polished": "3.0.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-helmet": "5.2.0",
    "react-modal": "3.8.1",
    "react-typography": "0.16.18",
    "rehype-react": "3.1.0",
    "typescript": "3.2.2",
    "typography": "0.16.18"
  },
  "devDependencies": {
    "husky": "1.2.1",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3"
  }
}
