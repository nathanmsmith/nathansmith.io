{
  "name": "nathansmith.io",
  "description": "Nathan Smith in HTML",
  "author": "Nathan Smith <nathan@nathansmith.io>",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "gatsby develop --open -p 1234",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "rm -rf node_modules/ .cache/ public/",
    "typegen": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,md,json,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@emotion/core": "10.0.10",
    "@emotion/styled": "10.0.12",
    "@fortawesome/fontawesome-svg-core": "1.2.19",
    "@fortawesome/free-brands-svg-icons": "5.9.0",
    "@fortawesome/free-solid-svg-icons": "5.9.0",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@graphql-codegen/cli": "1.2.1",
    "@mosch/gatsby-source-github": "0.4.1",
    "@types/react": "16.8.20",
    "@types/react-helmet": "5.0.8",
    "@types/react-modal": "3.8.2",
    "d3": "5.12.0",
    "dotenv": "8.0.0",
    "emotion": "10.0.9",
    "emotion-server": "10.0.9",
    "gatsby": "2.9.4",
    "gatsby-image": "2.1.4",
    "gatsby-plugin-emotion": "4.0.7",
    "gatsby-plugin-google-analytics": "2.0.21",
    "gatsby-plugin-manifest": "2.1.1",
    "gatsby-plugin-netlify": "2.0.17",
    "gatsby-plugin-netlify-cache": "1.2.0",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-remove-trailing-slashes": "2.0.11",
    "gatsby-plugin-sharp": "2.1.5",
    "gatsby-plugin-typescript": "2.0.15",
    "gatsby-plugin-typography": "2.3.14",
    "gatsby-remark-katex": "3.0.4",
    "gatsby-remark-prismjs": "3.2.11",
    "gatsby-remark-smartypants": "2.0.9",
    "gatsby-source-filesystem": "2.0.39",
    "gatsby-transformer-remark": "2.4.0",
    "gatsby-transformer-sharp": "2.1.21",
    "katex": "0.10.2",
    "polished": "3.4.1",
    "prismjs": "1.16.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-modal": "3.8.1",
    "react-typography": "0.16.19",
    "rehype-react": "3.1.0",
    "typescript": "3.5.2",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@graphql-codegen/typescript": "1.2.1",
    "@graphql-codegen/typescript-operations": "1.2.1",
    "@types/d3": "5.7.2",
    "husky": "3.0.8",
    "lint-staged": "9.2.0",
    "prettier": "1.18.2"
  }
}
