{
  "name": "nathansmith.io",
  "description": "Nathan Smith in HTML",
  "author": "Nathan Smith <nathan@nathansmith.io>",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "gatsby develop --open -p 1234",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "rm -rf node_modules/ .cache/ public/",
    "typegen": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,md,json,js,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.27",
    "@fortawesome/free-brands-svg-icons": "5.12.1",
    "@fortawesome/free-solid-svg-icons": "5.12.1",
    "@fortawesome/react-fontawesome": "0.1.9",
    "@fullhuman/postcss-purgecss": "1.3.0",
    "@graphql-codegen/cli": "1.13.0",
    "@mosch/gatsby-source-github": "0.4.1",
    "@types/react": "16.9.23",
    "@types/react-helmet": "6.1.0",
    "@types/react-modal": "3.10.5",
    "autoprefixer": "9.7.4",
    "dotenv": "8.2.0",
    "gatsby": "2.19.43",
    "gatsby-image": "2.2.43",
    "gatsby-plugin-feed": "2.3.28",
    "gatsby-plugin-google-analytics": "2.1.37",
    "gatsby-plugin-manifest": "2.2.47",
    "gatsby-plugin-netlify": "2.1.34",
    "gatsby-plugin-netlify-cache": "1.2.0",
    "gatsby-plugin-postcss": "2.1.21",
    "gatsby-plugin-react-helmet": "3.1.23",
    "gatsby-plugin-react-helmet-canonical-urls": "1.4.0",
    "gatsby-plugin-remove-trailing-slashes": "2.1.23",
    "gatsby-plugin-sass": "2.1.30",
    "gatsby-plugin-sharp": "2.4.12",
    "gatsby-plugin-typescript": "2.2.3",
    "gatsby-plugin-typography": "2.3.24",
    "gatsby-remark-autolink-headers": "2.1.25",
    "gatsby-remark-images": "3.1.49",
    "gatsby-remark-katex": "3.1.25",
    "gatsby-remark-prismjs": "3.3.35",
    "gatsby-remark-smartypants": "2.1.22",
    "gatsby-source-filesystem": "2.1.55",
    "gatsby-transformer-remark": "2.6.58",
    "gatsby-transformer-sharp": "2.3.18",
    "katex": "0.11.1",
    "node-sass": "4.13.1",
    "postcss-nested": "4.2.1",
    "prismjs": "1.19.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "6.1.0",
    "react-modal": "3.11.2",
    "react-typography": "0.16.19",
    "rehype-react": "4.0.1",
    "tailwindcss": "1.2.0",
    "typescript": "3.8.3",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@graphql-codegen/typescript": "1.13.0",
    "@graphql-codegen/typescript-operations": "1.13.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1"
  }
}
