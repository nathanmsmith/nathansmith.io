{
  "name": "nathansmith.io",
  "description": "Nathan Smith in HTML",
  "author": "Nathan Smith <nathan@nathansmith.io>",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "gatsby develop --open -p 1234",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "rm -rf node_modules/ .cache/ public/",
    "typegen": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,md,json,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@emotion/core": "10.0.9",
    "@emotion/styled": "10.0.9",
    "@fortawesome/fontawesome-svg-core": "1.2.17",
    "@fortawesome/free-brands-svg-icons": "5.8.1",
    "@fortawesome/free-solid-svg-icons": "5.8.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@graphql-codegen/cli": "1.0.3",
    "@types/react": "16.8.8",
    "@types/react-helmet": "5.0.8",
    "@types/react-modal": "3.8.1",
    "emotion": "10.0.9",
    "emotion-server": "10.0.9",
    "gatsby": "2.2.9",
    "gatsby-image": "2.0.34",
    "gatsby-plugin-emotion": "4.0.6",
    "gatsby-plugin-google-analytics": "2.0.17",
    "gatsby-plugin-manifest": "2.0.24",
    "gatsby-plugin-netlify": "2.0.13",
    "gatsby-plugin-netlify-cache": "1.1.0",
    "gatsby-plugin-react-helmet": "3.0.10",
    "gatsby-plugin-remove-trailing-slashes": "2.0.10",
    "gatsby-plugin-sharp": "2.0.30",
    "gatsby-plugin-typescript": "2.0.11",
    "gatsby-plugin-typography": "2.2.10",
    "gatsby-source-filesystem": "2.0.28",
    "gatsby-transformer-remark": "2.3.8",
    "gatsby-transformer-sharp": "2.1.17",
    "polished": "3.1.0",
    "react": "16.8.5",
    "react-dom": "16.8.5",
    "react-helmet": "5.2.0",
    "react-modal": "3.8.1",
    "react-typography": "0.16.19",
    "rehype-react": "3.1.0",
    "typescript": "3.3.4000",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@graphql-codegen/typescript": "1.0.3",
    "@graphql-codegen/typescript-operations": "1.0.3",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4"
  }
}
